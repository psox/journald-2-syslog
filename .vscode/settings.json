{
    "[rust]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 3,
        "editor.detectIndentation": false
    },
    "cSpell.words": [
        "Debounced",
        "Mutex",
        "Realtime",
        "chrono",
        "clippy",
        "deserialize",
        "journald",
        "journaldeliver",
        "journaldelivery",
        "serde",
        "stemmet",
        "syslog",
        "systemd",
        "usec",
        "vals"
    ],
    "editor.formatOnPaste": true,
    "editor.formatOnSaveTimeout": 100,
    "editor.formatOnType": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 7000,
    "files.hotExit": "onExitAndWindowClose",
    "projects.indentationSpaces": 3,
}